    body.index(class="{% if page.bodyclass %}{{ page.bodyclass }}{% endif %} {{ page.lang | prepend: 'lang-' }}")
        include includes/nav.jade
        header.header
            .container
                include includes/logo_clean.jade
            // .container end
        // header end

        div(class="triangle_reverse_container")
            svg(version="1.1", viewBox="0 0 770 63", class="triangle_reverse")
                path(d="M 769.0766,61.53043 0.5,61.629255 0.55974186,0.520315 C 0.23440393,30.221569 359.00793,14.210549 384.80814,45.029889 412.33508,14.210549 769.38188,30.328689 769.04308,0.5 z")
        .wrapper
            section.main_container
                .container
                    .content
                        {{content}}
                    // .content end
                // .container end
            // section end
        // .wrapper end
        include includes/footer.jade
        script(src="/js/build/main.min.js")
    // body end
// html end
